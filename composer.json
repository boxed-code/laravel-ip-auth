{
    "name": "boxed-code/laravel-ip-authorization",
    "description": "Authorize access based on IPv4 & IPv6 addresses, ranges, subnets or wildcards.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "green2go@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/framework": "^5.6|^6.0",
        "s1lentium/iptools": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BoxedCode\\Laravel\\Auth\\Ip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoxedCode\\Laravel\\Auth\\Ip\\IpAuthServiceProvider"
            ]
        }
    }
}
